CC = g++
CXXFLAGS = -std=c++11 -g -O2 -Wall
OBJS = TextRec.o Knn.o RankFusion.o test_TextRec_Ensemble.o
SRCS = TextRec/TextRec.cc Knn/Knn.cc RankFusion/RankFusion.cc test_TextRec_Ensemble.cc
LIB = -L ~/Kiwi/build -lkiwi
TARGET = Test_TextRec_Ensemble.out

$(TARGET) : $(OBJS)
	$(CC) -o $(TARGET) $(CXXFLAGS) $(OBJS) $(LIB)

$(OBJS) : $(SRCS)
	$(CC) $(CXXFLAGS) -c $(SRCS)

clean:
	rm -f *.o *.out
